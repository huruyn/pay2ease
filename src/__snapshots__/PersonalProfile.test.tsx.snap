// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`PersonalProfile Component matches the snapshot 1`] = `
<main>
  <div
    className="bg-light"
    style={
      Object {
        "backgroundImage": "url(Circle_White-01.jpg)",
        "backgroundSize": "cover",
      }
    }
  >
    <div
      className="spacing"
    />
    <div
      className="container"
    >
      <div
        className="row align-items-center pt-5"
      >
        <div
          className="col-12 col-md-3 formcurve"
          style={
            Object {
              "backgroundColor": "#FF7300",
            }
          }
        >
          <div
            className="row px-lg-4 text-center text-md-start"
          >
            <div
              className="col-12 mb-2 position-relative"
            >
              <img
                alt=""
                className="profilephoto rounded-circle position-absolute top-50 start-50 translate-middle"
                id="profilepicture"
                src="David_Magnus_White.jpg"
              />
              <img
                alt=""
                className="img-fluid position-absolute top-50 start-50 translate-middle "
                id="profilepicture"
                src="profileframe.png"
              />
            </div>
            <div
              className="spacing"
            />
            <div
              className="col-12 mb-4"
            >
              <h5
                className="fw-bold text-light text-center text-uppercase "
              >
                Profile Name
              </h5>
            </div>
            <div
              className="col-6 col-md-12 mb-3 text-center"
            >
              <a
                className="text-light text-uppercase text-decoration-none link-item"
                href="/"
                onClick={[Function]}
              >
                Contact Info
              </a>
            </div>
            <div
              className="col-6 col-md-12 mb-3 text-center"
            >
              <a
                className="text-light text-uppercase text-decoration-none link-item"
                href="/"
                onClick={[Function]}
              >
                Account Info
              </a>
            </div>
            <div
              className="col-6 col-md-12 mb-3 text-center"
            >
              <a
                className="text-light text-uppercase text-decoration-none link-item"
                href="/"
                onClick={[Function]}
              >
                Language
              </a>
            </div>
            <div
              className="col-6 col-md-12 mb-3 text-center"
            >
              <a
                className="text-light text-uppercase text-decoration-none link-item"
                href="/"
                onClick={[Function]}
              >
                profile.lorem_ipsum
              </a>
            </div>
            <div
              className="spacingsmall"
            />
          </div>
        </div>
        <div
          className="col-12 col-md-9"
        >
          <div
            className="cardbusiness formcurve border-0 p-4"
          >
            <div
              className="card-body"
            >
              <form
                action="#"
                method="post"
              >
                <div
                  className="row"
                >
                  <div
                    className="col-12 col-md-6 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="fname"
                    >
                      First Name
                    </label>
                    <input
                      className="form-control formcurve"
                      id="fname"
                      type="text"
                    />
                  </div>
                  <div
                    className="col-12 col-md-6 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="lname"
                    >
                      Last Name
                    </label>
                    <input
                      className="form-control formcurve"
                      id="lname"
                      type="password"
                    />
                  </div>
                  <div
                    className="col-12 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="Email"
                    >
                      Email
                    </label>
                    <input
                      className="form-control formcurve"
                      id="Email"
                      type="email"
                    />
                  </div>
                  <div
                    className="col-12 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="Address"
                    >
                      Address
                    </label>
                    <input
                      className="form-control formcurve"
                      id="Address"
                      type="email"
                    />
                  </div>
                  <div
                    className="col-12 col-md-6 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="city"
                    >
                      City
                    </label>
                    <input
                      className="form-control formcurve"
                      id="city"
                      type="text"
                    />
                  </div>
                  <div
                    className="col-12 col-md-6 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="province"
                    >
                      Province
                    </label>
                    <input
                      className="form-control formcurve"
                      id="province"
                      type="text"
                    />
                  </div>
                  <div
                    className="col-12 col-md-6 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="postalcode"
                    >
                      Postal Code
                    </label>
                    <input
                      className="form-control formcurve"
                      id="postalcode"
                      type="text"
                    />
                  </div>
                  <div
                    className="col-12 col-md-6 mb-4"
                  >
                    <label
                      className="text-light fw-bold"
                      htmlFor="submit"
                    >
                       
                      <input
                        className="w-100 py-2 btn btn-dark formcurve text-uppercase"
                        id="submit"
                        style={
                          Object {
                            "backgroundColor": "#5E7472",
                            "fontWeight": 700,
                          }
                        }
                        type="submit"
                      />
                    </label>
                  </div>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div
      className="spacing"
    />
  </div>
</main>
`;
