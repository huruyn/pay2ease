// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`SetUpDirectDepositWithCanadaRevenueAgency Component matches the snapshot 1`] = `
<div
  className="page-content"
>
  <div
    style={
      Object {
        "backgroundColor": "#F2F3F2",
        "padding": 10,
      }
    }
  >
    <p
      style={
        Object {
          "marginBottom": 0,
          "textAlign": "center",
        }
      }
    >
      Register for direct deposit with the Canada Revenue Agency
    </p>
  </div>
  <div
    className="mt-3"
  >
    <p>
      Please note that registering for direct deposit with the CRA doesn't automatically enrol you for government benefit programs.
      <a
        className="text-black"
        href="/"
      >
        Visit the Government of Canada website to apply for government benefits including COVID-19 relief benefits.
      </a>
    </p>
    <p>
      Direct deposit is a convenient way to receive your CAnada Revenue Agency payments and benefits.
    </p>
    <p>
      If you receive payments from the Canada Revenue Agency and have a Canadian bank account, you can sign up for direct deposit of certain payments, including:
    </p>
    <ul>
      <li>
        Canada Emergency Response Benefit(CERB)
      </li>
      <li>
        Income tax refund
      </li>
      <li>
        Goods and services tax or harmonized sales tax(GST/HST) credit
      </li>
      <li>
        Canada child benefit
      </li>
      <li>
        Working income tax benefit
      </li>
      <li>
        Ontario trillium benefit
      </li>
      <li>
        Canada Recovery Sickness Benefit(CRSB)
      </li>
      <li>
        Canada Recovery Caregiving Benefit(CRCB)
      </li>
      <li>
        Canada Recovery Benefit(CRB)
      </li>
    </ul>
  </div>
  <div>
    <h5
      className="pb-2 pt-2 mb-2 mt-3"
      style={
        Object {
          "borderBottom": "1px solid grey",
        }
      }
    >
      Personal information
    </h5>
    <p>
      To receive eligible payments, this information must match the information on file with the Canada Revenue Agency.
    </p>
    <div
      className="row"
    >
      <div
        className="col"
      >
        <label
          className="form-label"
          style={
            Object {
              "color": "grey",
              "fontSize": 14,
            }
          }
        >
          Legal name
        </label>
        <p>
          MR. DUANE TOUGH
        </p>
      </div>
    </div>
    <div
      className="row"
    >
      <div
        className="col-md-6"
      >
        <label
          className="form-label"
          style={
            Object {
              "color": "grey",
              "fontSize": 14,
            }
          }
        >
          Date of birth(Month/Day/Year)
        </label>
        <div
          className="row"
        >
          <div
            className="col-auto"
          >
            <select
              className="form-select"
            >
              <option>
                Month
              </option>
            </select>
          </div>
          <div
            className="col-auto"
          >
            <select
              className="form-select"
            >
              <option>
                Day
              </option>
            </select>
          </div>
          <div
            className="col-auto"
          >
            <select
              className="form-select"
            >
              <option>
                Year
              </option>
            </select>
          </div>
        </div>
      </div>
      <div
        className="col-md-6"
      >
        <label
          className="form-label"
          style={
            Object {
              "color": "grey",
              "fontSize": 14,
            }
          }
        >
          Social Insurance Number (9 digits)
        </label>
        <input
          className="form-control"
          type="text"
        />
      </div>
    </div>
  </div>
  <div>
    <h5
      className="pb-2 pt-2 mb-2 mt-3"
      style={
        Object {
          "borderBottom": "1px solid grey",
        }
      }
    >
      Set up direct deposit
    </h5>
    <p>
      Select the account you want to deposit the payments into.
    </p>
    <div
      className="row"
    >
      <div
        className="col-md-6"
      >
        <label
          className="form-label"
          style={
            Object {
              "color": "grey",
              "fontSize": 14,
            }
          }
        >
          Deposit account
        </label>
        <select
          className="form-select"
        >
          <option>
            Select an account
          </option>
        </select>
      </div>
    </div>
    <div
      className="mt-3 row"
    >
      <div
        className="col-md-12"
      >
        <div
          className="p-4"
          style={
            Object {
              "border": "1px solid grey",
            }
          }
        >
          <p>
            I authorize ZippyX to share my information, date of birth, Social Insurance Number(SIN) and full name with the Canada Revenue Agency (CRA) for the purposes of establishing direct deposit, including to identify me. By Providing my information to the CRA, I authorize the Receiver General to deposit in the bank account number provided, any amounts to be paid to me by the CRA, until the CRA is otherwise notified by me. I understand that this authorization will replace all of my previous direct deposit authorizations. I also acknowledge reading the Privacy Statement found below.
          </p>
        </div>
        <div
          className="p-4"
          style={
            Object {
              "background": "#a1a1a1",
              "color": "white",
            }
          }
        >
          <div
            className="form-check"
          >
            <input
              className="form-check-input"
              disabled={false}
              id="agree-send-information"
              name="agree-send-information"
              type="checkbox"
            />
            <label
              className="form-check-label"
              htmlFor="agree-send-information"
              title=""
            >
              Yes, Please send my information to the CRA to set up direct deposit
            </label>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div
    className="mt-4 row"
  >
    <div
      className="bg-light p-3 col-md-12"
      style={
        Object {
          "fontWeight": 500,
        }
      }
    >
      <span
        style={
          Object {
            "color": "rgb(220, 53, 69)",
            "marginRight": 5,
          }
        }
      >
        +
      </span>
      <span>
        Privacy Statement
      </span>
    </div>
  </div>
  <div
    className="mt-4"
  >
    <div
      className="mt-4 row"
    >
      <div
        className="col"
      >
        <button
          className="d-flex btn btn-light"
          disabled={false}
          type="button"
        >
          <div
            style={
              Object {
                "border": "1px dotted grey",
                "height": 25,
                "marginRight": 10,
                "textAlign": "center",
                "width": 20,
              }
            }
          >
            P
          </div>
          Cancel
        </button>
      </div>
      <div
        className="d-flex justify-content-end col"
      >
        <button
          className="zippy-btn d-flex btn btn-primary"
          disabled={false}
          type="button"
        >
          Submit
        </button>
      </div>
    </div>
  </div>
</div>
`;
